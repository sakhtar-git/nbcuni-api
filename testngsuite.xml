<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Nasa API Suite">
    <test name="Search API-Test">
        <classes>
            <class name="com.nbcuni.api.search.NasaTest">
                <methods>
                    <include name="verifyJsonResponseConformsToSchema"/>
                    <include name="verifySearch"/>
                    <include name="verifyWithAllParameters"/>
                    <include name="verifyWithIncorrectURI"/>
                    <include name="verifyWithInvalidParam"/>
                    <include name="verifyWithoutAnyParams"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>